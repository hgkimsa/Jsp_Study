<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.codenest.db.TodoMapper">
    
    <select id="getAllTodos" resultType="com.codenest.model.todoDTO">
        SELECT * FROM todo_list 
    </select>

    <select id="getTodosByAccount" parameterType="string" resultType="com.codenest.model.todoDTO">
        SELECT * FROM todo_list WHERE google_account = #{google_account}
    </select>
</mapper>